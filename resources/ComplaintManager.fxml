<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ComplaintManagerView">
   <children>
      <Pane prefHeight="400.0" prefWidth="600.0">
         <children>
            <TableView fx:id="tvComplaint" layoutX="135.0" layoutY="100.0" prefHeight="200.0" prefWidth="330.0">
              <columns>
                <TableColumn fx:id="accuserColumn" prefWidth="165.0" text="Accuser" />
                <TableColumn fx:id="complainantColumn" prefWidth="165.0" text="Complainant" />
              </columns>
            </TableView>
            <Button layoutX="223.0" layoutY="315.0" mnemonicParsing="false" text="Approve" onAction="#ApproveComplaint"/>
            <Button layoutX="322.0" layoutY="315.0" mnemonicParsing="false" text="Deny" onAction="#DenyComplaint"/>
            <Button layoutX="280.0" layoutY="350.0" mnemonicParsing="false" text="Back" onAction="#Back"/>
         </children>
      </Pane>
   </children>
</AnchorPane>
